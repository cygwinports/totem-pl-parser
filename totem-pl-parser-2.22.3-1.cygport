inherit gnome2

DESCRIPTION="GNOME playlist parser library"
HOMEPAGE="http://www.gnome.org/projects/totem/"

abi=10
PKG_NAMES="${PN} lib${PN/l-/l}${abi} lib${PN/l-/l}-devel"
PKG_HINTS="setup lib devel"
PKG_CONTENTS[0]="usr/share/doc/ usr/share/locale/"
PKG_CONTENTS[1]="usr/bin/*-${abi}.dll"
PKG_CONTENTS[2]="usr/include/ usr/lib/ usr/share/gtk-doc/"

DIFF_EXCLUDES="*.pc"

CYGCONF_ARGS="--without-hal"
